				if (ips[i] == string_null)
					break;
				else
				if (ips[i] == sender)
				{}
				else
					QC_SendPacket(ips[i], send_what);
				i++;
			}
			strunzone(send_what);
		}
		else
		if (argv(1) == "broadcast_globalmsg")
