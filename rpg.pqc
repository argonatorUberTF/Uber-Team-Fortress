void() RemoveVote =
{
	newmis = spawn();
	newmis.classname = "VoteFailed";
	newmis.nextthink = time + 10;  //was 60, to hurry things up on a failed vote.
	newmis.think = RemoveVote2;
	nomap = 0;
};
void() RemoveVote2 =
{
	if (votestarted == 0)
	{
		dremove(self);
		return;
	}
	prozacmethod = 0;
	local entity head;
	if (nextmapchosen != 1)
	{
		localcmd("say -- VOTE HAS FAILED --");
	}
	votestarted = 0;
	num_votes = 0;
	self.startedvote = 0;
	self.vote_wait = (time + 10);
	head = nextent(world);
	while (head != world)
	{
		if (head.classname == "player")
		{
			head.has_voted = 0;
		}
	head = nextent(head); 
	}
	return;
	dremove (self);
};
