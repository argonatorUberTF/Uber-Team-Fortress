void (string sender) Master_UpdateConnections =
{
	local float i, found_me;
	i=0;
	while (i<STORED_ADDRESSES)
	{
		if (ips[i] == string_null)
			break;
		else
		if (ips[i] == sender)
		{
			found_me = 1;
			break;
		}
		i++;
	}
	if (!found_me)
	{
		i=0;
		while (i<STORED_ADDRESSES)
		{
			if (ips[i] == string_null)
			{
				ips[i] = sender;
				ips[i] = strzone(ips[i]);
				bprint(2,ips[i]);
				bprint(2," added to the IP roster\n");
				break;
			}
			i++;
		}
	}
};
