		TeamFortress_SetSpeed(self);
		self.heat = 0;
	}
	te = find(world, classname, "timer");
	while (te != world)
	{
		if (te.owner == self)
		{
			dremove(te);
			te = find(world, classname, "timer");
		}
		else
		{
			te = find(te, classname, "timer");
		}
	}
	te = find(world, classname, "item_tfgoal");
	while (te)
	{
		if (te.owner == self)
