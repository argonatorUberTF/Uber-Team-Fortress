	{
		if (te.touch == ammo_touch || te.touch == weapon_touch)
		{
			te.solid = TF_FLARE_LIT;
			te.enemy = self.owner;
			te.nextthink = time + TF_FLARE_OFF + random() * 2;
			te.think = EMPExplode;
