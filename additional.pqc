{
			if (head.classname == "monster_zombie" && head.is_monster == 1)
			{
deathmsg = 30;
TF_T_Damage(head, self, self.owner, 200, TF_FLARE_LIT, 4);
			}
			head = head.chain;
		}
	te = findradius(self.origin, 240);
