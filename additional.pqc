					bprint(2, "  the  flag!\n");
				}
			}
		}
		te = find(te, classname, "item_tfgoal");
	}
	te = find(world, classname, "detpack");
	while (te)
	{
		if (te.weaponmode == 1 && te.enemy == self)
		{
			te.weaponmode = 0;
		}
		te = find(te, classname, "detpack");
	}
	te = find(world, classname, "pipebomb");
	while (te != world)
	{
