void() Sentry_Rotate = 
{
	local float ay;
	CheckSentry(self);
	if (Sentry_FindTarget())
	{
		return;
	}
	if (self.heat == 0)
	{
		self.angles_x = 0;
		self.ideal_yaw = self.waitmin;
		ChangeYaw();
		ay = anglemod(self.angles_y);
		ay = rint(ay);
		if (ay == rint(self.waitmin))
		{
			self.heat = 1;
			if (random() < 0.1)
			{
				sound(self, 3, "weapons/gunidle1.wav", 0.5, 1);
			}
		}
	}
	else
	{
		self.ideal_yaw = self.waitmax;
		ChangeYaw();
		ay = anglemod(self.angles_y);
		ay = rint(ay);
		if (ay == rint(self.waitmax))
		{
			self.heat = 0;
		}
	}
};
