	spot2 = targ.origin + targ.view_ofs;
	if (self.#is_minion == #TRUE && targ == self.owner)		// follow owner, don't shoot him
		return 0;
	traceline(spot1, spot2, TF_FLARE_OFF, self);
	if (trace_inopen && trace_inwater)
	{
		return TF_FLARE_LIT;
	}
